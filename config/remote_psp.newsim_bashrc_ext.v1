hn=$(hostname)
x=$(host -t txt $hn)
echo $x | grep "rnsc=1" >/dev/null
if [[ $? == 0 ]]; then
    echo "$(date) setting local environment for remote PSPs" >> /tmp/remote_psp.log
    export ATC_JAVA6=/opt/ufa/java/...
    export ATC_JAVA_BIN=/opt/ufa/java/...
    export ATC_JAVA=/opt/ufa/java/...
    export ATC_JAVA6_BIN=/opt/ufa/java/...
fi
